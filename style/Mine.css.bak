.ul {
    list-style: none;
    padding: 0;
    margin: 0;
}
.card-list-item{
    border-bottom: 1px solid #eee;
    padding-top: 12px;
    position: relative;
}
.card-list-item:hover {
        background: #F0F9FF
}
.info-area{
    float: right;
}
.info-area li {
    float: left;
    margin-right: 15px;
    font-size: 12px;
    height: 34px;
    line-height: 34px;
}
.category-name{
    width: 88px;
    height: 34px;
    line-height: 34px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #fff;
    background: #FF835F;
    text-align: right;
    padding: 0 5px;
    margin-right: 10px;
    font-size: 12px;
}
.fields-area{
    padding: 20px 50px;
}
.fields-area li{
    margin-right: 30px;
    overflow: hidden;
    white-space: nowrap;
    max-width: 50%;
    float: left;
    text-overflow: ellipsis;
    font-size: 14px;
    line-height: 35px;
    color:#666;
}
.toolbar-area{
    display: none;
    position: absolute;
    right: 2px;
    bottom: 2px;
    padding: 6px 0;
    background: #C6E5FF;
}
.card-list-item:hover .toolbar-area{
    display: block;
}
.toolbar-area li {
    float: left;
    border-right: 1px solid #81C3FF
}
.toolbar-area li:last-child {
    border: 0 none;
}
.toolbar-area li a {
    color: #0799E9;
    text-decoration: none;
    line-height: 20px;
    height: 20px;
    font-size: 12px;
    display: block;
    width: 75px;
    text-align: center;
}
.h3 {
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #333;
    font-size: 15px;
    margin: 0;
    margin-right: 310px;
    vertical-align: middle;
    font-weight: normal;
}
.h3 span {
    display: inline-block;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle;
    height: 34px;
    line-height: 34px;
}
.notSubmit{
    color: #FF6600;
}
.inHandle{
    color: #0799E7;
}
.complate{
    color: #00DA8F;
}
